Cell["\<\
(* The goal of the `Manipulate` code is to dynamically illustrate and compare \
the
numerical approximations of the first derivative of the function f(x)= sin(x) \
at
x0=Pi/4 using finite difference methods (forward, backward, and central
differences) with a variable step size h. The code calculates these \
approximations
and the exact derivative, displays the results, and plots the function along \
with
the tangent lines corresponding to the numerical approximations. The \
interactive
slider allows users to adjust the step size h, providing a visual and numerical
understanding of how the step size affects the accuracy of the derivative
approximations: *)\
\>", "comment",
 CellChangeTimes->{3.94391336484947*^9},
 CellID->811132638,ExpressionUUID->"1ba77d21-8850-442f-9228-c70d758732c1"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Manipulate", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "f", ",", "x0", ",", "forwardDifference", ",", "backwardDifference", 
       ",", "centralDifference", ",", "exactDerivate", ",", "plotFunction"}], 
      "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"f", "[", "x_", "]"}], ":=", 
       RowBox[{"Sin", "[", "x", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"x0", "=", 
       RowBox[{"\[Pi]", "/", "4"}]}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"forwardDifference", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"f", "[", 
           RowBox[{"x0", "+", "h"}], "]"}], "-", 
          RowBox[{"f", "[", "x0", "]"}]}], ")"}], "/", "h"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"backwardDifference", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"f", "[", "x0", "]"}], "-", 
          RowBox[{"f", "[", 
           RowBox[{"x0", "-", "h"}], "]"}]}], ")"}], "/", "h"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"centralDifference", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"f", "[", 
           RowBox[{"x0", "+", "h"}], "]"}], "-", 
          RowBox[{"f", "[", 
           RowBox[{"x0", "-", "h"}], "]"}]}], ")"}], "/", 
        RowBox[{"(", 
         RowBox[{"2", "h"}], ")"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"exactDerivate", "=", 
       RowBox[{
        RowBox[{"D", "[", 
         RowBox[{
          RowBox[{"f", "[", "x", "]"}], ",", "x"}], "]"}], "/.", 
        RowBox[{"x", "->", "x0"}]}]}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"Column", "[", "\[IndentingNewLine]", 
       RowBox[{"{", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"Row", "[", 
          RowBox[{"{", 
           RowBox[{"\"\<Step size (h): \>\"", ",", 
            RowBox[{"NumberForm", "[", 
             RowBox[{"h", ",", 
              RowBox[{"{", 
               RowBox[{"4", ",", "2"}], "}"}]}], "]"}]}], "}"}], "]"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"Row", "[", 
          RowBox[{"{", 
           RowBox[{"\"\<Forward Difference Approximation: \>\"", ",", 
            RowBox[{"NumberForm", "[", 
             RowBox[{"forwardDifference", ",", 
              RowBox[{"{", 
               RowBox[{"8", ",", "6"}], "}"}]}], "]"}]}], "}"}], "]"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"Row", "[", 
          RowBox[{"{", 
           RowBox[{"\"\<Backward Difference Approximation: \>\"", ",", 
            RowBox[{"NumberForm", "[", 
             RowBox[{"backwardDifference", ",", 
              RowBox[{"{", 
               RowBox[{"8", ",", "6"}], "}"}]}], "]"}]}], "}"}], "]"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"Row", "[", 
          RowBox[{"{", 
           RowBox[{"\"\<Central Difference Approximation: \>\"", ",", 
            RowBox[{"NumberForm", "[", 
             RowBox[{"centralDifference", ",", 
              RowBox[{"{", 
               RowBox[{"8", ",", "6"}], "}"}]}], "]"}]}], "}"}], "]"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"Row", "[", 
          RowBox[{"{", 
           RowBox[{"\"\<Exact Derivate: \>\"", ",", 
            RowBox[{"NumberForm", "[", 
             RowBox[{"exactDerivate", ",", 
              RowBox[{"{", 
               RowBox[{"8", ",", "6"}], "}"}]}], "]"}]}], "}"}], "]"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"Plot", "[", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"{", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"f", "[", "x", "]"}], ",", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"f", "[", "x0", "]"}], "+", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{"x", "-", "x0"}], ")"}], "forwardDifference"}]}], ",",
              "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"f", "[", "x0", "]"}], "+", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{"x", "-", "x0"}], ")"}], "backwardDifference"}]}], 
             ",", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"f", "[", "x0", "]"}], "+", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{"x", "-", "x0"}], ")"}], "centralDifference"}]}]}], 
            "}"}], ",", "\[IndentingNewLine]", 
           RowBox[{"{", 
            RowBox[{"x", ",", 
             RowBox[{"x0", "-", 
              RowBox[{"2", "h"}]}], ",", 
             RowBox[{"x0", "+", 
              RowBox[{"2", "h"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
           RowBox[{"PlotStyle", "->", 
            RowBox[{"{", 
             RowBox[{"Blue", ",", "Red", ",", "Green", ",", "Purple"}], 
             "}"}]}], ",", "\[IndentingNewLine]", 
           RowBox[{"PlotLegends", "->", 
