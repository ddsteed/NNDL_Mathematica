Cell["\<\
(* The code demonstrates the numerical approximation of the first derivative of
the function f(x)= sin(x) at x0=Pi/4 using finite difference methods. It \
calculates
the forward, backward, and central difference approximations with a step size
h=0.1,and compares these results to the exact derivative obtained analytically.
The code prints the approximations and the exact derivative, and generates a \
plot
of the function over the interval [x_0-h,x_0+h], highlighting the point at x0
where the derivative is approximated: *)\
\>", "comment",
 CellChangeTimes->{3.9439129655956984`*^9},
 CellID->610925311,ExpressionUUID->"2bf4cfcc-cba8-4771-b940-03a59f39c6f1"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"f", "[", "x_", "]"}], ":=", 
  RowBox[{"Sin", "[", "x", "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"x0", "=", 
   RowBox[{"\[Pi]", "/", "4"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"h", "=", "0.1"}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"forwardDifference", "=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"f", "[", 
       RowBox[{"x0", "+", "h"}], "]"}], "-", 
      RowBox[{"f", "[", "x0", "]"}]}], ")"}], "/", "h"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"backwardDifference", "=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"f", "[", "x0", "]"}], "-", 
      RowBox[{"f", "[", 
       RowBox[{"x0", "-", "h"}], "]"}]}], ")"}], "/", "h"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"centralDifference", "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"f", "[", 
        RowBox[{"x0", "+", "h"}], "]"}], "-", 
       RowBox[{"f", "[", 
        RowBox[{"x0", "-", "h"}], "]"}]}], ")"}], "/", 
     RowBox[{"(", 
      RowBox[{"2", "h"}], ")"}]}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"exactDerivate", "=", 
    RowBox[{
     RowBox[{"D", "[", 
      RowBox[{
       RowBox[{"f", "[", "x", "]"}], ",", "x"}], "]"}], "/.", 
     RowBox[{"x", "->", "x0"}]}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{"Print", "[", 
  RowBox[{
  "\"\<Forward Difference Approximation: \>\"", ",", "forwardDifference"}], 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{"Print", "[", 
  RowBox[{
  "\"\<Backward Difference Approximation: \>\"", ",", "backwardDifference"}], 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{"Print", "[", 
  RowBox[{
  "\"\<Central Difference Approximation: \>\"", ",", "centralDifference"}], 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Print", "[", 
   RowBox[{"\"\<Exact Derivative: \>\"", ",", 
    RowBox[{"N", "[", "exactDerivate", "]"}]}], "]"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"plotFunction", "=", 
    RowBox[{"Plot", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"f", "[", "x", "]"}], ",", "\[IndentingNewLine]", 
      RowBox[{"{", 
       RowBox[{"x", ",", 
        RowBox[{"x0", "-", "h"}], ",", 
        RowBox[{"x0", "+", "h"}]}], "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{"PlotStyle", "->", "Blue"}], ",", "\[IndentingNewLine]", 
      RowBox[{
      "PlotLabel", "->", "\"\<Function and Numerial Approximations\>\""}], 
      ",", "\[IndentingNewLine]", 
      RowBox[{"Epilog", "->", 
       RowBox[{"{", "\[IndentingNewLine]", 
        RowBox[{"Red", ",", "\[IndentingNewLine]", 
         RowBox[{"PointSize", "[", "Large", "]"}], ",", "\[IndentingNewLine]", 
         RowBox[{"Point", "[", 
          RowBox[{"{", 
           RowBox[{"x0", ",", 
            RowBox[{"f", "[", "x0", "]"}]}], "}"}], "]"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"Text", "[", 
          RowBox[{"\"\<x0\>\"", ",", 
           RowBox[{"{", 
            RowBox[{"x0", ",", 
             RowBox[{"f", "[", "x0", "]"}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"1", ",", 
             RowBox[{"-", "1"}]}], "}"}]}], "]"}]}], "\[IndentingNewLine]", 
        "}"}]}], ",", "\[IndentingNewLine]", 
      RowBox[{"AxesLabel", "->", 
       RowBox[{"{", 
        RowBox[{"\"\<x\>\"", ",", "\"\<f[x]\>\""}], "}"}]}]}], 
     "\[IndentingNewLine]", "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{"Show", "[", "plotFunction", "]"}]}], "Input",
